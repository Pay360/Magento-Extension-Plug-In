<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Pay360_Payments::config_pay360_payments" title="pay360"/>
						</resource>
					</resource>
				</resource>
				<resource id="Pay360_Payments::Profile" sortOrder="10" title="Profile">
					<resource id="Pay360_Payments::Profile_save" sortOrder="10" title="Save Profile"/>
					<resource id="Pay360_Payments::Profile_delete" sortOrder="20" title="Delete Profile"/>
					<resource id="Pay360_Payments::Profile_update" sortOrder="30" title="Update Profile"/>
					<resource id="Pay360_Payments::Profile_view" sortOrder="40" title="View Profile"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
